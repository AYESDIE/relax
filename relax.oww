rule("Size Toggle UP")
{
	event
	{
		Ongoing - Each Player;
		All;
		All;
	}

	conditions
	{
		Event Player.CHILL == True;
		Event Player.SCALING == False;
		Is Button Held(Event Player, Button(Ability 2)) == True;
		Is Button Held(Event Player, Button(Primary Fire)) == False;
		Is Button Held(Event Player, Button(Secondary Fire)) == True;
	}

	actions
	{
		Event Player.SCALE_SIZE += 1;
		Event Player.SCALE_SIZE %= 20;
		Event Player.SCALING = True;
	}
}

rule("Size Toggle DOWN")
{
	event
	{
		Ongoing - Each Player;
		All;
		All;
	}

	conditions
	{
		Event Player.CHILL == True;
		Event Player.SCALING == False;
		Is Button Held(Event Player, Button(Ability 2)) == True;
		Is Button Held(Event Player, Button(Primary Fire)) == True;
		Is Button Held(Event Player, Button(Secondary Fire)) == False;
	}

	actions
	{
		Event Player.SCALE_SIZE -= 1;
		Skip If(Event Player.SCALE_SIZE >= 0, 1);
		Event Player.SCALE_SIZE = 19;
		Event Player.SCALING = True;
	}
}

