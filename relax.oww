disabled rule("GLOBAL")
{
	event
	{
		Ongoing - Global;
	}
}

rule("Workshop Variables Override")
{
	event
	{
		Ongoing - Global;
	}

	actions
	{
		"Time is in seconds."
		Global.MATCH_TIME = Workshop Setting Integer(Custom String("Global"), Custom String("Match Time (in Seconds)"), 1800, 300, 3600,
			0);
		Global.SLO_MO_ENABLE = Workshop Setting Toggle(Custom String("Plugins"), Custom String("Slo Mo"), False, 0);
		Global.CUSTOM_BOT_ENABLE = Workshop Setting Toggle(Custom String("Plugins"), Custom String("Custom Bot"), False, 0);
	}
}

